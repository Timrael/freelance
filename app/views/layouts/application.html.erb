<!DOCTYPE html>
<html>
<head>
  <title>Freelance</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

<% flash.each do |name, msg| %>
    <div class="<%= name %>">
      <p><%= msg %></p>
    </div>
<% end %>

<%= yield %>

</body>
</html>
